{
  "name": "marianne",
  "version": "1.0.0",
  "description": "Multipurpose Discord.js bot built with Klasa",
  "main": "src/Marianne.js",
  "author": "KunoichiZ",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc -b src",
    "watch": "tsc -w",
    "start": "node dist/index.js",
    "dev": "run-s build start",
    "watch:start": "tsc-watch -b src --onSuccess \"node ./dist/index.js\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@sapphire/decorators": "^4.3.8",
    "@sapphire/discord-utilities": "next",
    "@sapphire/discord.js-utilities": "next",
    "@sapphire/fetch": "^2.4.1",
    "@sapphire/framework": "next",
    "@sapphire/plugin-api": "next",
    "@sapphire/plugin-editable-commands": "next",
    "@sapphire/plugin-logger": "next",
    "@sapphire/plugin-subcommands": "next",
    "@sapphire/time-utilities": "^1.7.5",
    "@sapphire/type": "^2.2.4",
    "@sapphire/utilities": "^3.9.1",
    "colorette": "^2.0.19",
    "discord-api-types": "^0.33.5",
    "discord.js": "^13.10.2",
    "dotenv-cra": "^3.0.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@sapphire/prettier-config": "^1.4.3",
    "@sapphire/ts-config": "^3.3.4",
    "@types/node": "^18.7.8",
    "@types/ws": "^8.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  },
  "imports": {
    "#data/*": "./dist/lib/data/*.js",
    "#lib/*": "./dist/lib/*.js",
    "#root/*": "./dist/*.js",
    "#utils/*": "./dist/lib/util/*.js"
  },
  "prettier": "@sapphire/prettier-config",
  "bugs": {
    "url": "https://github.com/hildamari/Marianne/issues"
  },
  "repository": "https://github.com/hildamari/Marianne.git",
  "homepage": "https://hilda.pw",
  "keywords": [
    "discord",
    "discordjs",
    "discord-bot",
    "klasa",
    "marianne",
    "fire-emblem-three-houses",
    "fire-emblem",
    "three-houses"
  ]
}
